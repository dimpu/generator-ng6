{
  "name": "generator-ng6",
  "version": "1.0.0",
  "description": "angualr component devlopment pack with jspm,es6,bootstrap",
  "dependencies": {},
  "scripts": {
    "start": "node scripts/serve",
    "build:jspm": "jspm bundle-sfx src/app/app.js dist/app.js",
    "build:annotate": "ng-annotate --add dist/app.js -o dist/app.js --sourcemap --sourceroot 'app.js.map'",
    "build:uglify": "uglifyjs dist/app.js -o dist/app.min.js",
    "build:html": "node scripts/build",
    "build": "npm run build:jspm && npm run build:annotate && npm run build:uglify && npm run build:html",
    "test": "karma start",
    "postinstall": "jspm install"
  },
  "keywords": [
    "angular",
    "jspm",
    "es6"
  ],
  "author": "Aravind Buddha",
  "license": "ISC",
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.4.7",
      "angular-mocks": "npm:angular-mocks@^1.4.7",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.15",
      "clean-css": "npm:clean-css@^3.4.4",
      "css": "github:systemjs/plugin-css@^0.1.11",
      "json": "github:systemjs/plugin-json@^0.1.0",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.25",
      "babel-runtime": "npm:babel-runtime@^5.8.25",
      "core-js": "npm:core-js@^1.2.1"
    }
  },
  "devDependencies": {
    "browser-sync": "^2.7.6",
    "change-case": "^2.3.0",
    "jspm": "^0.16.11",
    "karma": "^0.12.36",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jspm": "^1.1.5",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "ng-annotate": "^1.0.2",
    "node-useref": "^0.3.15",
    "yargs": "^3.10.0"
  }
}
